datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Student {
  id         Int      @id @default(autoincrement())
  name       String
  surname    String
  email      String   @unique
  password   String
  age        Int
  created_at DateTime
  updated_at DateTime
}

model Users {
  id         Int      @id @default(autoincrement())
  name       String
  email      String   @unique
  password   String
  created_at DateTime
  updated_at DateTime
}
