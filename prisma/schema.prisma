datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Student {
  id         Int       @id @default(autoincrement())
  name       String
  surname    String
  email      String    @unique
  age        Int
  created_at DateTime?
  updated_at DateTime?
}
